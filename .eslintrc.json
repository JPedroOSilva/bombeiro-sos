{   
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "standard" ,"airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
        "react/jsx-filename-extension": [
          "error",
          {
            "extensions": [".js", ".jsx"]
          }
        ],
        "global-require": "off",
        "import/prefer-default-export": "off",
        "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
      }
}